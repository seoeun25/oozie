<bundle-app name='bundle-fs' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xmlns='uri:oozie:bundle:0.1'>
    <controls>
        <kick-off-time>${kickOffTime}</kick-off-time>
    </controls>
    <coordinator name='coordCreateDir' >
        <app-path>${coordCreateAppPath}</app-path>
        <configuration>
            <property>
                <name>startTime</name>
                <value>${startTime1}</value>
            </property>
            <property>
                <name>endTime</name>
                <value>${endTime1}</value>
            </property>
        </configuration>
    </coordinator>
    <coordinator name='coordDeleteDir' >
        <app-path>${coordDeleteAppPath}</app-path>
        <configuration>
            <property>
                <name>startTime</name>
                <value>${startTime2}</value>
            </property>
            <property>
                <name>endTime</name>
                <value>${endTime2}</value>
            </property>
        </configuration>
    </coordinator>
</bundle-app>